name: demos
version: 0.1.0.0
cabal-version: >=1.8
build-type: Simple
license: AllRightsReserved
license-file: ""
synopsis: SFML demos
category: Game
author: Marc Sunet
data-dir: ""
 
executable window
    hs-source-dirs:     window
    build-depends:      base, SFML
    main-is:            main.hs
    buildable:          True
    ghc-options:        -debug
    extra-libraries:    csfml-window-d csfml-system-d

executable time
    hs-source-dirs:     time
    build-depends:      base, SFML
    main-is:            main.hs
    buildable:          True
    ghc-options:        -debug
    extra-libraries:    csfml-system-d

executable transform
    hs-source-dirs:     transform
    build-depends:      base, SFML
    main-is:            main.hs
    buildable:          True
    ghc-options:        -debug
    extra-libraries:    csfml-system-d csfml-graphics-d

